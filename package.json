{
    "name": "vscode-h2o",
    "displayName": "Shell Script command completion",
    "description": "Add CLI autocomplete to Shell Script",
    "repository": {
        "url": "https://github.com/yamaton/vscode-h2o"
    },
    "publisher": "tetradresearch",
    "author": {
        "name": "Yamato Matsuoka",
        "email": "yamato.matsuoka@tetradresearch.com"
    },
    "license": "MIT",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.58.1"
    },
    "categories": [
        "Snippets"
    ],
    "icon": "images/animal_chara_computer_penguin.png",
    "keywords": [
        "shellscript",
        "completion",
        "autocomplete",
        "intellisense",
        "cli",
        "bioinformatics",
        "man",
        "shell",
        "bash",
        "h2o"
    ],
    "activationEvents": [
        "onLanguage:shellscript",
        "onCommand:h2o.clearCache",
        "onCommand:h2o.loadCommon",
        "onCommand:h2o.loadBio",
        "onCommand:h2o.removeBio"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "h2o.clearCache",
                "title": "Shell Completion: Clear Cache Of..."
            },
            {
                "command": "h2o.loadCommon",
                "title": "Shell Completion: Load Common CLI Data"
            },
            {
                "command": "h2o.loadBio",
                "title": "Shell Completion: Load Bioinformatics CLI Data"
            },
            {
                "command": "h2o.removeBio",
                "title": "Shell Completion: Remove Bioinformatics CLI Data"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "h2o.clearCache",
                    "when": "editorLangId == shellscript"
                },
                {
                    "command": "h2o.loadCommon",
                    "when": "editorLangId == shellscript"
                },
                {
                    "command": "h2o.loadBio",
                    "when": "editorLangId == shellscript"
                },
                {
                    "command": "h2o.removeBio",
                    "when": "editorLangId == shellscript"
                }
            ]
        },
        "configuration": {
            "title": "Shell Completion",
            "properties": {
                "h2o.h2oPath": {
                    "type": "string",
                    "default": "<bundled>",
                    "description": "Path to the H2O executable. Enter <bundled> if using the bundled."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.11.13",
        "@types/node-fetch": "^2.5.12",
        "@types/pako": "^1.0.2",
        "@types/vscode": "^1.58.1",
        "@typescript-eslint/eslint-plugin": "^5.7.0",
        "@typescript-eslint/parser": "^5.7.0",
        "eslint": "^8.4.1",
        "glob": "^7.2.0",
        "mocha": "^9.1.3",
        "tree-sitter-bash": "^0.19.0",
        "tree-sitter-cli": "^0.20.1",
        "typescript": "^4.5.4",
        "vscode-test": "^1.6.1"
    },
    "dependencies": {
        "node-fetch": "^2.6.6",
        "pako": "^2.0.4",
        "web-tree-sitter": "^0.19.4"
    }
}
