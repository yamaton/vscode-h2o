{
    "name": "vscode-h2o",
    "displayName": "Shell Completion",
    "description": "Enable shell completion in shell script mode",
    "repository": {
        "url": "https://github.com/yamaton/vscode-h2o"
    },
    "publisher": "tetradresearch",
    "author": {
        "name": "Yamato Matsuoka",
        "email": "yamato.matsuoka@tetradresearch.com"
    },
    "license": "MIT",
    "version": "0.0.8",
    "engines": {
        "vscode": "^1.57.0"
    },
    "categories": [
        "Snippets",
        "Other"
    ],
    "keywords": [
        "cli",
        "man",
        "shell",
        "bash",
        "completion",
        "intellisense",
        "h2o"
    ],
    "activationEvents": [
        "onLanguage:shellscript",
        "onCommand:h2o.clearCache"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "h2o.clearCache",
                "title": "H2O: Clear Cache"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "h2o.clearCache",
                    "when": "editorLangId == shellscript"
                }
            ]
        },
        "configuration": {
            "title": "h2o",
            "properties": {
                "h2o.h2oPath": {
                    "type": "string",
                    "default": "<bundled>",
                    "description": "Path to the H2O executable. Enter <bundled> if using the bundled one."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "^15.12.4",
        "@types/node-fetch": "^2.5.10",
        "@types/pako": "^1.0.1",
        "@types/vscode": "^1.57.0",
        "@typescript-eslint/eslint-plugin": "^4.28.0",
        "@typescript-eslint/parser": "^4.28.0",
        "eslint": "^7.29.0",
        "glob": "^7.1.7",
        "mocha": "^9.0.1",
        "tree-sitter-bash": "^0.19.0",
        "tree-sitter-cli": "^0.19.5",
        "typescript": "^4.3.4",
        "vscode-test": "^1.5.2"
    },
    "dependencies": {
        "node-fetch": "^2.6.1",
        "pako": "^2.0.3",
        "web-tree-sitter": "^0.19.4"
    }
}
